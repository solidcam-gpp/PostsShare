    endif
    if iSaveTurretNumber<<ChannelID>> eq 3                                     
      sSpindle = '3'
      iSpinMCode<<ChannelID>> = 80
    endif
    if iSaveTurretNumber<<ChannelID>> eq 4                                     
      sSpindle = '3'
