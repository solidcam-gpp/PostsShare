      {nl,'O',iSubProgNumber:'z4.0(p)','(JOB-NR.',ToStr(index_job:integer_def_f),')'}
      call @usr_ReplacedString(original_job_name)
      {nl,'(',original_job_name,')'}
    endif
    if iMode eq 0 and iTrans4xCount<<ChannelID>> eq 0
      {nl,'M99'}
      {nl,'%'}
      {nl,'!!close file = ''!!'} 
    endif
  endif
endp
                                          
