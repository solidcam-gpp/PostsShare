  sMessage<<5>> = 'MCO-Kommentar ist nicht an erster Stelle' + sJob
  ;Gewaehlter Koordinatentyp kartesisch wird nicht unterstuetzt
  sMessage<<6>> = 'Der gewaehlte Koordinatentyp "Kartesisch"' + sJob + 'wird nicht unterstuetzt!'
